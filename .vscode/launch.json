{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Package",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "args": [
        "run",
        "--config",
        "config.yaml",
        "-l",
        "file",
        "--loaderFile",
        "config.yaml"
      ]
    },
    {
      "name": "Gen md docs",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "args": [
        "gen-docs",
        "--path",
        "docs"
      ]
    }
  ]
}